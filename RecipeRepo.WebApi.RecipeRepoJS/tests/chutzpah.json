{
    "Framework": "jasmine",

    "Compile": {
        "Extensions": [ ".ts" ],
        "ExtensionsWithNoOutput": [ ".d.ts" ],
        "Mode": "External"
    },

    "References": [
        {
            "Excludes": [ "*.intellisense.js", "*.min.js", "*.map", "*jasmine.js*", "*jasmine-html.js*", "*boot.js*" ],
            "Includes": [ "*.js" ],
            "Path": "../scripts"
        },
        {
            "Includes": [ "*mockController.js" ]
        },
        {
            "Path": "../lib",
            "Includes": [ "*.js" ]
        }
    ],
    "Tests": [
        {
            "Includes": [ "*Tests.ts" ]
        }
    ]
}